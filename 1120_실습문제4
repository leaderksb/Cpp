#include <iostream>
using namespace std;

bool bigger(int a, int b, int& big) {
	if (a == b)
		return true;
	else
	{
		if (a > b)
		big = a;
		else
		big = b;
		return false;
	}
	
};

int main() {
	int x, y, max = 0;
	cout << "두 수를 입력 : ";
	cin >> x >> y;
	if (true) {
		cout << "두 수가 같다." << endl;
	}
	if (false) {
		cout << "두 수가 다르다." << endl;
		cout << "더 큰 수는" << max << endl;
	}
	return 0;
}
